LIB	= @LIB_TARGET@

all:	$(LIB)

clean::
	@- $(RM) -r autom4te.cache
	- $(RM) *.*cov *.da *.bb *.bbg
	- $(LIBTOOL_CLEAN) $(RM) *.i *$o *.a *.so*
	- $(RM) core *~ tags TAGS

distclean:	clean
	$(RM) makefile config.h config.cache config.log config.status

.c.o:
	@RULE_CC@
	@ECHO_CC@$(LIBTOOL_COMPILE) $(CC) $(CFLAGS) $(CPPFLAGS) -c $<

